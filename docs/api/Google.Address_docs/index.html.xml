        <function name="SearchAddress" scope="static" constructor="false" deprecated="false" private="false" protected="false" ignored="false" internal="false" type="Function">
            <description>The SearchAddress function takes a map, options and callback function. The options can contain either an address string, to which a point is returned - or reverse geocoding a GLatLng, where an address is returned</description>
            <parameters>
                <parameter name="element" type="jQuery">
                    <description>The jQuery object containing the map element.</description>
                </parameter>
                <parameter name="options" type="Object">
                    <description>An object of options</description>
                </parameter>
                <parameter name="callback" type="Function">
                    <description>The callback function that returns the result</description>
                </parameter>
            </parameters>
            <return-types>
                <return-type type="Function" />
            </return-types>
            <function name="defaults" scope="static" constructor="false" deprecated="false" private="false" protected="false" ignored="false" internal="false" type="Object">
                <description>Default options for SearchAddress</description>
                <parameters>
                    <parameter name="query" type="String">
                        <description>The Address or GLatLng to query in the geocoder</description>
                    </parameter>
                    <parameter name="returnType" type="String">
                        <description>The type of value you want to return from Google. This is mapped to the function names available, the options are &apos;getLatLng&apos; which returns coordinates, and &apos;getLocations&apos; which returns points.</description>
                    </parameter>
                    <parameter name="cache" type="GGeoCache">
                        <description>The GGeoCache to store the results in if required</description>
                    </parameter>
                    <parameter name="countryCode" type="String">
                        <description>The country code to localise results</description>
                    </parameter>
                </parameters>
                <aliases>
                    <alias name="Mapifies.SearchAddress.defaults" />                </aliases>
                <return-types>
                    <return-type type="Object" />
                </return-types>
                <property name="&apos;query&apos;" scope="static" constructor="false" deprecated="false" private="false" protected="false" ignored="true" internal="false" type="null">
                </property>
                <property name="&apos;returnType&apos;" scope="static" constructor="false" deprecated="false" private="false" protected="false" ignored="true" internal="false" type="String">
                </property>
                <property name="&apos;cache&apos;" scope="static" constructor="false" deprecated="false" private="false" protected="false" ignored="true" internal="false" type="Object">
                </property>
                <property name="&apos;countryCode&apos;" scope="static" constructor="false" deprecated="false" private="false" protected="false" ignored="true" internal="false" type="String">
                </property>
            </function>
