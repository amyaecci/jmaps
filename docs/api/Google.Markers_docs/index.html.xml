        <function name="CreateMarkerManager" scope="static" constructor="false" deprecated="true" private="false" protected="false" ignored="false" internal="false" type="Function">
            <description>This function allows you to create a marker manager to store and manage any markers created on the map. Google recommends not using this marker manager and instead using the open source one.</description>
            <parameters>
                <parameter name="element" type="jQuery">
                    <description>The element to initialise the map on.</description>
                </parameter>
                <parameter name="options" type="GMarker">
                    <description>The marker to be removed</description>
                </parameter>
                <parameter name="callback" type="Function">
                    <description>The callback function to pass out after initialising the map.</description>
                </parameter>
            </parameters>
