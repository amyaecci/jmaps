            </return-types>
            <function name="defaults" scope="static" constructor="false" deprecated="false" private="false" protected="false" ignored="false" internal="false" type="Object">
                <description>Default options for CreateMarkerManager</description>
                <parameters>
                    <parameter name="borderPadding" type="Number">
                        <description>Specifies, in pixels, the extra padding outside the map&apos;s current viewport monitored by a manager. Markers that fall within this padding are added to the map, even if they are not fully visible.</description>
                    </parameter>
                    <parameter name="maxZoom" type="Number">
                        <description>The maximum zoom level to show markers at</description>
                    </parameter>
                    <parameter name="trackMarkers" type="Boolean">
                        <description>Indicates whether or not a marker manager should track markers&apos; movements.</description>
                    </parameter>
