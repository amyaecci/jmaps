        <function name="AddMarker" scope="static" constructor="false" deprecated="false" private="false" protected="false" ignored="false" internal="false" type="Function">
            <description>This function allows you to add markers to the map with several options</description>
            <parameters>
                <parameter name="element" type="jQuery">
                    <description>The element to initialise the map on.</description>
                </parameter>
                <parameter name="options" type="Object">
                    <description>The object that contains the options.</description>
                </parameter>
                <parameter name="callback" type="Function">
                    <description>The callback function to pass out after initialising the map.</description>
                </parameter>
            </parameters>
            <aliases>
                <alias name="Mapifies.AddMarker" />            </aliases>
            <return-types>
                <return-type type="Function" />
            </return-types>
            <function name="defaults" scope="static" constructor="false" deprecated="false" private="false" protected="false" ignored="false" internal="false" type="Object">
                <description>Default options for AddGroundOverlay</description>
                <parameters>
                    <parameter name="pointLatLng" type="Object">
                        <description>The Lat/Lng coordinates of the marker.</description>
                    </parameter>
                    <parameter name="pointHTML" type="String">
                        <description>The HTML to appear in the markers info window.</description>
                    </parameter>
                    <parameter name="pointOpenHTMLEvent" type="String">
                        <description>The javascript event type to open the marker info window. Default is &apos;click&apos;.</description>
                    </parameter>
                    <parameter name="pointIsDraggable" type="Boolean">
                        <description>Defines if the point is draggable by the end user. Default false.</description>
                    </parameter>
                    <parameter name="pointIsRemovable" type="Boolean">
                        <description>Defines if the point can be removed by the user. Default false.</description>
                    </parameter>
                    <parameter name="pointRemoveEvent" type="Boolean">
                        <description>The event type to remove a marker. Default &apos;dblclick&apos;.</description>
                    </parameter>
                    <parameter name="pointMinZoom" type="Number">
                        <description>The minimum zoom level to display the marker if using a marker manager.</description>
                    </parameter>
                    <parameter name="pointMaxZoom" type="Number">
                        <description>The maximum zoom level to display the marker if using a marker manager.</description>
                    </parameter>
                    <parameter name="pointIcon" type="GIcon">
                        <description>A GIcon to display instead of the standard marker graphic.</description>
                    </parameter>
                    <parameter name="centerMap" type="Boolean">
                        <description>Automatically center the map on the new marker. Default false.</description>
                    </parameter>
                </parameters>
                <aliases>
                    <alias name="Mapifies.AddGroundOverlay.defaults" />                </aliases>
                <return-types>
